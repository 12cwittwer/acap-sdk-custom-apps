{
  "schemaVersion": "1.7.2",
  "acapPackageConf": {
    "setup": {
      "friendlyName": "Parkspass QR Scanner",
      "appName": "ParkspassQRScanner",
      "vendor": "Axis Communications",
      "embeddedSdkVersion": "3.0",
      "vendorUrl": "https://www.axis.com",
      "runMode": "respawn",
      "version": "1.0.0"
    },
    "configuration": {
      "paramConfig": [
        {
          "name": "ENDPOINT",
          "default": "https://trailwaze.info/parkspass/api/fastPassScan.php",
          "type": "string"
        },
        {
          "name": "ENTRANCE",
          "default": "1",
          "type": "string"
        },
        {
          "name": "LOCATION",
          "default": "UTSNOW",
          "type": "string"
        },
        {
          "name": "AUTH",
          "default": "PARKSPLUS",
          "type": "string"
        }
      ]
    }
  }
}
